{
  "name": "goodish",
  "version": "0.0.9",
  "description": "A collection of utility functions like common Math functions and common iterator operations",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "nyc mocha test/**/*.spec.ts",
    "lint": "tslint --fix --project .",
    "prepare": "tsc && npm run docs",
    "docs": "typedoc --out docs ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wyattis/goodish.git"
  },
  "keywords": [
    "utility",
    "math",
    "iteration",
    "random",
    "seed"
  ],
  "author": "wyattis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wyattis/goodish/issues"
  },
  "homepage": "https://github.com/wyattis/goodish#readme",
  "devDependencies": {
    "@types/chai": "^4.2.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "async-mutex": "^0.1.4",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.15.0",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typescript": "^3.6.2"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "src/**/*.d.ts"
    ],
    "reporter": [
      "text"
    ]
  }
}
